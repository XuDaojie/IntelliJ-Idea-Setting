<templateSet group="Java Me">
  <template name="mc" value="&#10;* @author $USER$&#10;* @date $DATE$" description="method comment" toReformat="false" toShortenFQNames="true">
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
  <template name="pvm" value="&#10;/**&#10; * @author $USER$&#10; * @date $DATE$&#10; * @version 1&#10; */&#10;private void $METHOD_NAME$() {&#10;    // todo&#10;}" description="privite void method" toReformat="false" toShortenFQNames="true">
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="METHOD_NAME" expression="complete()" defaultValue="method" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="cc" value="&#10;// @author $USER$&#10;// @date $DATE$" description="code momment" toReformat="false" toShortenFQNames="true">
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="cct" value="&#10;// @author $USER$&#10;// @date $DATE$&#10;// tag::$TAG$&#10;&#10;// end::$TAG$" description="code momment tag" toReformat="false" toShortenFQNames="true">
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="TAG" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>